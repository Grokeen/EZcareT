                                              ,REC.ADS_STF_NO                                                                                              --026.비선택의직원번호 APIPLIST.SPCDR2
                                              ,REC.ADS_STF_SID                                                                                             --027.비선택의직원식별ID
                                              ,V_ACPPRARD.ICD10_CD                                                                                         --028.ICD10코드
                                              ,V_ACPPRARD.ML_ORD_CD                                                                                        --029.식사처방코드
                                              ,V_ML_ORD_CD                                                                                                 --030.끼니구분코드
                                              ,''                                                                                                          --031.엄마환자번호
                                              ,''                                                                                                          --032.보호자유무
                                              ,'3'                                                                                                         --033.예상재원일수
